outliers_mean=function(sample){
m = mean(sample)
s = sd(sample)
outliers = vector()
j = 0
for(i in 1:length(sample))
if(sample[i] <m - 2*s | sample[i] > m + 2*s){
j = j + 1
outliers[j] = sample[i]
}
return(outliers)
}
