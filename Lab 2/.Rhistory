outliers_mean <- function(file) {
# Citim valorile din fișier
sample <- scan(file)
mean_val <- mean(sample)
sd_val <- sd(sample)
lower_limit <- mean_val - 2 * sd_val
upper_limit <- mean_val + 2 * sd_val
outliers <- sample[sample < lower_limit | sample > upper_limit]
return(outliers)
}
outliers_mean <- function(file) {
# Citim valorile din fișier
sample <- scan(file)
q1 <- quantile(sample, 0.25)
q3 <- quantile(sample, 0.75)
iqr <- q3 - q1
lower_limit <- q1 - 1.5 * iqr
upper_limit <- q3 + 1.5 * iqr
outliers <- sample[sample < lower_limit | sample > upper_limit]
return(outliers)
}
